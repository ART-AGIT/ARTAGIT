<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.artagit.web.dao.PayListDao">
	<select id="getList" resultType="com.artagit.web.entity.PayList">
		select * 
		from PayListView
		where exhId = #{id}
	</select>
</mapper>