<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.artagit.web.dao.ReviewDao">
	
	<select id="get" resultType="com.artagit.web.entity.Review">
		select *
		from Review
		where id =#{id}
	</select>
</mapper>
 